{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "types": ["cypress", "node", "reflect-metadata", "@cypress/grep"],
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./"
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts", "cypress/support/commands.js", "cypress/support/e2e.js"]
}
